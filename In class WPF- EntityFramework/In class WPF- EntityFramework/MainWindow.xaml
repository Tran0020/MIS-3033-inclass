<Window x:Class="In_class_WPF__EntityFramework.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:In_class_WPF__EntityFramework"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnfetch" Content="Fetch Data" HorizontalAlignment="Left" Margin="543,95,0,0" VerticalAlignment="Top" Width="153" RenderTransformOrigin="0.5,0.5" Height="68" Click="btnfetch_Click">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.844"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <ListBox x:Name="lstData" HorizontalAlignment="Left" Height="387" Margin="10,10,0,0" VerticalAlignment="Top" Width="513"/>
        <TextBox x:Name="txtstudentid" HorizontalAlignment="Left" Height="23" Margin="665,38,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Student ID:" HorizontalAlignment="Left" Margin="597,38,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
